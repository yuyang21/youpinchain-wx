.income_details .no_list {
  display: flex;
  align-items: center;
  justify-content: center;
  min-height: 600rpx;
  color: #999;
  font-size: 30rpx;
}

.income_details .ul {
  overflow: hidden;
  background-color: #fff;
  width: 100%;
  clear: both;
}

.income_details .ul .li {
  width: 100%;
  height: 110rpx;
  border-bottom: 2rpx solid #efeff4;
  line-height: 110rpx;
  padding: 0 24rpx;
  clear: both;
  overflow: hidden;
}

.income_details .ul .li view {
  float: left;
  font-size: 30rpx;
  color: #666;
  width: 32%;
}

.income_details .ul .li view:nth-child(2) {
  width: 48%;
}

.income_details .ul .li view:last-child {
  color: #e4372e;
  text-align: right;
  width: 20%;
}