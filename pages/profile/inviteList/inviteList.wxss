.inviteList {
  background-color: #efeff4;
  position: relative;
  padding-bottom: 100rpx;
}

.inviteList .no_list {
  display: flex;
  align-items: center;
  justify-content: center;
  min-height: 1100rpx;
  font-size: 30rpx;
  color: #999;
  background-color: #f6f5f5;
}

.inviteList .content {
  overflow: auto;
  width: 100%;
  background-color: #fff;
}

.inviteList .content .li {
  width: 25%;
  height: 155rpx;
  float: left;
  border-bottom: 2rpx solid #efeff4;
  display: flex;
  align-items: center;
  justify-content: center;
}

.inviteList .content .li image {
  height: 76%;
  width: 61%;
  vertical-align: middle;
  background-color: #efeff4;
  border-radius: 50%;
  font-size: 24rpx;
}

.inviteList .content .li image.noImage {
  width: 113rpx;
}

.inviteList .invite_btn {
  width: 100%;
  height: 90rpx;
  font-size: 30rpx;
  color: #fff;
  position: fixed;
  bottom: 0;
  background: #fc5340;
  line-height: 92rpx;
  text-align: center;
  margin: 160rpx auto 0;
}

.poster {
  position: absolute;
  top: 0;
  z-index: 9999;
  width: 100vw;
  height: 100vh;
  background: rgba(0, 0, 0, 1);
  padding-top: 40rpx;
}

.poster .close_model {
  position: absolute;
  bottom: 60rpx;
  left: 47%;
  width: 60rpx;
  height: 60rpx;
}

.poster .p {
  font-size: 30rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #fff;
  text-align: center;
  font-weight: bold;
}

.poster .invite_landing {
  overflow: hidden;
  position: relative;
  top: 60rpx;
  bottom: 0;
  left: 0;
  right: 0;
  margin: 0 auto;
  background: rgba(0, 0, 0, 0.8);
  width: 432rpx;
  height: 768rpx;
}

.poster .invite_landing image {
  /* display: block;
  position: relative;
  right: -2rpx;
  bottom: -4rpx; */
  width: 100%;
  height: 768rpx;
}

.poster .invite_landing .box {
  position: relative;
  width: 100%;
  height: 100%;
}

.poster .invite_landing .box .boxImg {
  width: 432rpx;
  height: 768rpx;
  position: absolute;
  left: 0;
  top: 0;
}

.poster .invite_landing .box .container {
  position: relative;
  z-index: 111;
  margin: 0 auto;
  padding-top: 224rpx;
  width: 80%;
  text-align: center;
}

.poster .invite_landing .box .container .title {
  font-size: 22rpx;
  color: #333;
  height: 72rpx;
  transform: scale(0.95) translate(40rpx, 10rpx);
}

.poster .invite_landing .box .container .content {
  font-size: 22rpx;
  color: #333;
  transform: scale(0.8);
  width: 88%;
  margin-left: 6%;
}

.poster .invite_landing .box .container .qrcode {
  width: 240rpx;
  height: 240rpx;
  border-radius: 10rpx;
  background: #000;
  margin: 24rpx 52rpx;
}
canvas {
  width:432rpx;
  height:768rpx;
}