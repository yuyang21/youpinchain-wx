@import '../group/groupDet/groupDet.wxss';
.goods .cart_btn_alone, .goods .add_cart_btn {
  width: 50%;
}
.standardBox {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: #efeff4;
  border-radius: 16rpx 16rpx 0 0;
  height: 80%;
  width: 100%;
  z-index: 4;
  overflow: scroll;
  padding-bottom: 100rpx;
}
.standardBox .btn,
.low_stocks_btn {
  position: fixed;
  bottom: 0;
  background-color: #fff;
  border-top: 2rpx solid #ddd;
  width: 100%;
  height: 100rpx;
  display:flex;
  justify-content:center;
  align-items:center;
}
.standardBox .btn .sureBtn,
.low_stocks_btn button {
    width: 100%;
    height: 100rpx;
    line-height:100rpx;
    color: #fff;
    font-size: 30rpx;
    background-color: #e4372e;
    border-radius: 0;
    border: none;
}
.low_stocks_btn button {
  background-color: #999;
}
.standardBox .btn .leftBtn, .standardBox .btn .rightBtn {
  width: 50%;
  height: 100rpx;
  line-height:100rpx;
  color: #fff;
  font-size: 30rpx;
  background-color: #f6cd4f;
  border-radius: 0;
  border: none;
}
.standardBox .btn .rightBtn {
  background-color: #de4a40;
}
.standardBox .closeBox {
  width: 110rpx;
  height: 110rpx;
  position: fixed;
  right: 0;
  top: 20%;
  background-color: #fff;
  padding:30rpx;
  z-index: 2;
}
.standardBox .close {
  background-color: #fff;
  border: 4rpx solid #ddd;
  border-radius: 50%;
  color: #ddd;
  width: 50rpx;
  height: 50rpx;
  line-height: 40rpx;
  font-weight: bold;
  text-align: center;
  display: block;
}
.standardBox .content .standards {
  overflow: hidden;
  width: 100%;
  padding: 0rpx 10% 30rpx;
  background-color: #fff;
}
.standardBox .content .standard .left, 
.standardBox .standards .price .left,
.standardBox .standards .number .left {
  width: 120rpx;
  font-size: 24rpx;
  color: #999;
}
.standardBox .content .standard .right,
.standardBox .standards .price .right,
.standardBox .standards .number .right {
  width: 78%;
  font-size: 24rpx;
  color: #000;
  overflow: hidden;
}
.standardBox .content .standard .right .li {
  /* height: 35rpx; */
  line-height: 35rpx;
  font-size: 24rpx;
  color: #000;
  text-align: center;
  border: 2rpx solid #ddd;
  border-radius: 6rpx;
  float: left;
  padding: 7rpx 10rpx;
  margin: 0 30rpx 30rpx 0;
}
.standardBox .content .standard .right .li.active {
  color: #e4372e;
  border: 2rpx solid #e4372e;
}
.standardBox .standards .standard,
.standardBox .standards .price,
.standardBox .standards .number {
  margin-bottom: 45rpx;
  overflow: hidden;
}
.standardBox .standards .standard {
  margin: 100rpx 0 15rpx;
}
.standardBox .standards .prices {
  position:fixed;
  width:100%;
  background:#fff;
  height: autp;
  padding-top: 30rpx;
}
.standardBox .standards .price {
  height: 50rpx;
}
.standardBox .standards .price .right span {
  color: #e4372e;
  font-weight: bold;
  font-size: 30rpx;
}
.cartIcon {
  position: fixed;
  right: 0;
  bottom: 10%;
  width: 50rpx;
  height: 60rpx;
  background-color: #e4372e;
  display: flex;
  align-items: center;
}
.cartIcon image {
  height: 100%;
  width: 60rpx;
  margin-left: -30rpx;
}
.cartIcon span {
  color: #e4372e;
  font-size: 26rpx;
  display: block;
  width: 25rpx;
  height: 25rpx;
  line-height: 25rpx;
  position: absolute;
  background: #fff;
  border-radius: 50%;
  text-align: center;
  right: 5rpx;
  top: 5rpx;
}
.mask_box {
  background-color: rgba(0, 0, 0, 0.6);
  width: 100%;
  height: 0;
  position: fixed;
  top: 0;
  z-index: 3;
}
.security {
    background-color:#fff;
    margin-top:30rpx;
}
.security .ul {
    overflow: hidden;
    padding: 8px 27px;
    padding: 15rpx 50rpx;
}
.security .ul .li {
    width: 33.33%;
    float: left;
    text-align: center;
    margin-bottom: 20rpx;
}
.security .ul .li span {
    display: block;
    font-size: 24rpx;
    color: #333;
}
.security .ul .li image {
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
}
.standardBox .number {
  overflow: hidden;
  width: 100%;
}
.standardBox .number .subduction image, .standardBox .number .add image {
  width: 35rpx;
  height: 35rpx;
}
.standardBox .number .num {
  display: inline-block;
  text-align: center;
  width: 100rpx;
  height: 38rpx;
  font-size: 24rpx;
  color: #333;
  vertical-align: top;
  border:2rpx solid #ddd;
  margin: 0 20rpx;
}
.top_main .standard {
  background-color: #fff;
  overflow: hidden;
  padding:30rpx 0 30rpx 30rpx;
  border-top:2rpx solid #ddd;
}
.top_main .standard .left {
  width: 17%;
  font-size: 24rpx;
  color: #333;
}
.top_main .standard .right {
  width: 83%;
}
.top_main .standard .right .li {
  font-size: 24rpx;
  color: #333;
}

.explain-title {
  text-align:center;
  font-size:18px;
  margin:16px 0;
  color:red;
}
.explain-content {
  font-size:13px;
  padding-top:0px;
  line-height:20px;
  padding:3px 20px;
}
.spell_list .panel_title {
  border-left: none;
}
.spell_list .loadMore {
  padding-right: 15rpx;
  color: #999;
  font-size: 26rpx;
}
.spell_list .loadMore image {
  width:13rpx;
  height:25rpx;
  position:relative;
  top:3rpx;
  right:-2rpx;
}
.spell_list .list {
  overflow: hidden;
  padding: 0 15rpx;
  height: 335rpx;
}
.spell_list .list .li {
  overflow: hidden;
  padding: 30rpx 0;
  height:110rpx !important;
  border-bottom:2rpx solid #eee;
}
.spell_list .list .li image {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}
.spell_list .list .li .nickName {
  font-size: 30rpx;
  color: #000;
  margin: 32rpx 0 0 10rpx;
}
.spell_list .list .li .text {
  font-size: 28rpx;
  color: #333;
  text-align: right;
  margin: 15rpx 30rpx 0 0;
}
.spell_list .list .li .text .countDown {
  font-size: 24rpx;
  color: #666;
}
.spell_list .list .li .btn {
  font-size: 28rpx;
  color: #fff;
  background-color: #e4372e;
  border-radius: 6rpx;
  text-align: center;
  line-height: 60rpx;
  height: 60rpx;
  width: 140rpx;
  margin-top: 28rpx;
}
.spell_box {
  width: 86%;
  height: 60%;
  background-color: #fff;
  border-radius: 10rpx;
  position: fixed;
  top: 10%;
  left: 7%;
  z-index: 4;
}
.spell_box .title {
  text-align: center;
  color: #000;
  font-size: 30rpx;
  height: 100rpx;
  line-height: 64rpx;
  border-bottom: 2rpx solid #eee;
}
.spell_box .close {
  position:absolute;
  right:-4%;
  top:-4%;
  background-color:#eee;
  color:#999;
  font-size:30rpx;
  width:50rpx;
  height:50rpx;
  line-height:46rpx;
  border-radius:50%;
  text-align:center;
}
.spell_box .list {
  padding: 10rpx 20rpx;
  box-sizing:border-box;
  height:86%;
}
.spell_box .list .li {
  overflow: hidden;
  border-bottom:2rpx solid #fdfdfd;
  margin-bottom: 30rpx;
}
.spell_box .list .li:last-child {
  border: none;
}
.spell_box .list .li image {
  width: 120rpx;
  height: 120rpx;
  margin-right: 15rpx;
  border-radius: 50%;
}
.spell_box .list .li .btn {
  font-size: 26rpx;
  color: #fff;
  background-color: #e4372e;
  border-radius: 6rpx;
  text-align: center;
  line-height: 50rpx;
  height: 50rpx;
  width: 120rpx;
  margin-top: 28rpx;
}
.spell_box .list .li .nickName {
  font-size: 30rpx;
  color: #000;
  margin-right: 30rpx;
}
.spell_box .list .li .text {
  font-size: 28rpx;
  color: #666;
  text-align: right;
  margin: 15rpx 30rpx 0 0;
}
.spell_box .list .li .text .countDown {
  font-size: 24rpx;
  color: #666;
  text-align:left;
}